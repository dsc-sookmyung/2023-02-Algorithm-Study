#include<iostream>
#include<algorithm>
using namespace std;

int main() {
	string num, palindrome;

	cin >> num;

	while (num != "0") {
		palindrome = num;

		reverse(palindrome.begin(), palindrome.end());

		if (num == palindrome) {
			cout << "yes\n";
		}
		else {
			cout << "no\n";
		}

		cin >> num;
	}

	return 0;
}